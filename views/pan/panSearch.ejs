<!DOCTYPE html>
<html>
<head>
    <% include ../template/header.ejs %>
    <title><%= title %></title>
    <style>
        .container-fluid {
            height: 100%;
        }
    </style>
</head>
<body>
<% include ../template/nav %>
<div class="container">
    <div class="row">
        <div class="col-md-5">
            <form class="form-inline" action="" method="post">
                <div class="form-group"><input type="text" name="q"/></div>
                <div class="form-group"><input type="submit" value="搜索"/></div>
            </form>
            <% if(data){ %>
            <% data.forEach(function(item){ %>
            <div>
                <a class="btn btn-link" href="<%= item.href %>" target="_blank">
                    <%= item.content %>
                </a>
            </div>
            <% }); %>
            <% } %>
        </div>
    </div>
</div>
<% include ../template/footer.ejs %>
</body>
</html>